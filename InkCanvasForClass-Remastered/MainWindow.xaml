<Window
    x:Class="InkCanvasForClass_Remastered.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Controls="http://schemas.microsoft.com/netfx/2009/xaml/presentation"
    xmlns:Settings="clr-namespace:InkCanvasForClass_Remastered.Models"
    xmlns:controls="clr-namespace:InkCanvasForClass_Remastered.Controls"
    xmlns:converters="clr-namespace:InkCanvasForClass_Remastered.Converters"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:ikw="http://schemas.inkore.net/lib/ui/wpf"
    xmlns:local="clr-namespace:InkCanvasForClass_Remastered"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:ui="http://schemas.inkore.net/lib/ui/wpf/modern"
    xmlns:viewModels="clr-namespace:InkCanvasForClass_Remastered.ViewModels"
    Name="window"
    Title="InkCanvasForClass-Remastered"
    Width="900"
    Height="400"
    d:DataContext="{d:DesignInstance viewModels:MainViewModel}"
    AllowsTransparency="True"
    Background="Transparent"
    Closed="Window_Closed"
    Closing="Window_Closing"
    DpiChanged="MainWindow_OnDpiChanged"
    FontFamily="Microsoft YaHei UI"
    FontSize="14"
    Foreground="Black"
    KeyDown="Window_KeyDown"
    Loaded="Window_Loaded"
    MouseWheel="Window_MouseWheel"
    PreviewKeyDown="Main_Grid_PreviewKeyDown"
    ResizeMode="NoResize"
    ShowInTaskbar="False"
    SizeChanged="MainWindow_OnSizeChanged"
    SourceInitialized="Window_SourceInitialized"
    Stylus.IsFlicksEnabled="False"
    Stylus.IsPressAndHoldEnabled="False"
    Stylus.IsTapFeedbackEnabled="False"
    Stylus.IsTouchFeedbackEnabled="False"
    Topmost="True"
    WindowStyle="None"
    mc:Ignorable="d">
    <!--  资源中添加命令  -->
    <Window.Resources>
        <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
        <converters:IsEnabledToOpacityConverter x:Key="IsEnabledToOpacityConverter" />
        <converters:IntNumberToString x:Key="IntNumberToString" />
        <converters:IntNumberToString2 x:Key="IntNumberToString2" />
        <converters:BoolToBrushConverter x:Key="BoolToBrushConverter" />
        <converters:BoolToOpacityConverter x:Key="BoolToOpacityConverter" />
        <converters:BoolToScaleConverter x:Key="BoolToScaleConverter" />
        <converters:EnumToVisibilityConverter x:Key="EnumToVisibilityConverter" />
        <RoutedUICommand x:Key="KeyExit" Text=" " />
        <RoutedUICommand x:Key="HotKey_Command_Undo" Text=" " />
        <RoutedUICommand x:Key="HotKey_Command_Redo" Text=" " />

        <Style x:Key="ToolPanelButtonStyle" TargetType="Button">
            <Setter Property="Width" Value="32" />
            <Setter Property="Height" Value="38" />
            <Setter Property="Background" Value="Transparent" />
            <Setter Property="Cursor" Value="Hand" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Button">
                        <Border
                            x:Name="border"
                            Background="{TemplateBinding Background}"
                            BorderThickness="0">
                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                        </Border>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter TargetName="border" Property="Background" Value="#11000000" />
                            </Trigger>
                            <Trigger Property="IsPressed" Value="True">
                                <Setter TargetName="border" Property="Background" Value="#1C18181B" />
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <Style x:Key="SidePanelViewBoxStyle" TargetType="Viewbox">
            <Setter Property="Visibility" Value="Collapsed" />
            <Style.Triggers>
                <Trigger Property="Tag" Value="Left">
                    <Setter Property="Margin" Value="-60,0,0,-150" />
                </Trigger>
                <Trigger Property="Tag" Value="Right">
                    <Setter Property="Margin" Value="0,0,-10,-150" />
                </Trigger>
                <MultiDataTrigger>
                    <MultiDataTrigger.Conditions>
                        <Condition Binding="{Binding IsFloatingBarVisible}" Value="False" />
                        <Condition Binding="{Binding RelativeSource={RelativeSource Mode=Self}, Path=Tag}" Value="Left" />
                    </MultiDataTrigger.Conditions>
                    <Setter Property="Visibility" Value="Visible" />
                    <MultiDataTrigger.EnterActions>
                        <BeginStoryboard>
                            <Storyboard>
                                <ThicknessAnimation
                                    Storyboard.TargetProperty="Margin"
                                    From="-60,0,0,-150"
                                    To="-10,0,0,-150"
                                    Duration="0:0:0.175">
                                    <ThicknessAnimation.EasingFunction>
                                        <CubicEase EasingMode="EaseInOut" />
                                    </ThicknessAnimation.EasingFunction>
                                </ThicknessAnimation>
                            </Storyboard>
                        </BeginStoryboard>
                    </MultiDataTrigger.EnterActions>
                </MultiDataTrigger>
                <MultiDataTrigger>
                    <MultiDataTrigger.Conditions>
                        <Condition Binding="{Binding IsFloatingBarVisible}" Value="False" />
                        <Condition Binding="{Binding RelativeSource={RelativeSource Mode=Self}, Path=Tag}" Value="Right" />
                    </MultiDataTrigger.Conditions>
                    <Setter Property="Visibility" Value="Visible" />
                    <MultiDataTrigger.EnterActions>
                        <BeginStoryboard>
                            <Storyboard>
                                <ThicknessAnimation
                                    Storyboard.TargetProperty="Margin"
                                    From="0,0,-60,-150"
                                    To="0,0,-10,-150"
                                    Duration="0:0:0.175">
                                    <ThicknessAnimation.EasingFunction>
                                        <CubicEase EasingMode="EaseInOut" />
                                    </ThicknessAnimation.EasingFunction>
                                </ThicknessAnimation>
                            </Storyboard>
                        </BeginStoryboard>
                    </MultiDataTrigger.EnterActions>
                </MultiDataTrigger>
            </Style.Triggers>
        </Style>
    </Window.Resources>
    <!--  输入命令绑定  -->
    <Window.InputBindings>
        <KeyBinding Command="{StaticResource KeyExit}" Gesture="Escape" />
        <KeyBinding
            Key="Z"
            Command="{StaticResource HotKey_Command_Undo}"
            Modifiers="Control" />
        <KeyBinding
            Key="Y"
            Command="{StaticResource HotKey_Command_Redo}"
            Modifiers="Control" />
    </Window.InputBindings>
    <!--  命令执行方法绑定  -->
    <Window.CommandBindings>
        <CommandBinding
            CanExecute="CommandBinding_CanExecute"
            Command="{StaticResource KeyExit}"
            Executed="KeyExit" />
        <CommandBinding
            CanExecute="CommandBinding_CanExecute"
            Command="{StaticResource HotKey_Command_Undo}"
            Executed="HotKey_Undo" />
        <CommandBinding
            CanExecute="CommandBinding_CanExecute"
            Command="{StaticResource HotKey_Command_Redo}"
            Executed="HotKey_Redo" />
    </Window.CommandBindings>

    <Grid x:Name="Main_Grid">

        <!--  // 设置界面 //  -->
        <Grid
            x:Name="BorderSettingsMask"
            Panel.ZIndex="999"
            IsHitTestVisible="{Binding IsSettingsPanelVisible}"
            MouseDown="SettingsOverlayClick">
            <Grid.Style>
                <Style TargetType="Grid">
                    <Setter Property="Background" Value="{x:Null}" />
                    <Style.Triggers>
                        <DataTrigger Binding="{Binding IsSettingsPanelVisible}" Value="True">
                            <Setter Property="Background" Value="#01000000" />
                        </DataTrigger>
                    </Style.Triggers>
                </Style>
            </Grid.Style>
            <Border
                Name="BorderSettings"
                Width="440"
                HorizontalAlignment="Left"
                ui:ThemeManager.RequestedTheme="Dark"
                Background="#ee18181b">
                <Border.Style>
                    <Style TargetType="Border">
                        <Setter Property="Margin" Value="-440,0,0,0" />
                        <Style.Triggers>
                            <DataTrigger Binding="{Binding IsSettingsPanelVisible}" Value="True">
                                <Setter Property="Margin" Value="0,0,0,0" />
                                <DataTrigger.EnterActions>
                                    <BeginStoryboard>
                                        <Storyboard>
                                            <ThicknessAnimation
                                                FillBehavior="Stop"
                                                Storyboard.TargetProperty="Margin"
                                                From="-440,0,0,0"
                                                To="0,0,0,0"
                                                Duration="0:0:0.6">
                                                <ThicknessAnimation.EasingFunction>
                                                    <CubicEase EasingMode="EaseOut" />
                                                </ThicknessAnimation.EasingFunction>
                                            </ThicknessAnimation>
                                        </Storyboard>
                                    </BeginStoryboard>
                                </DataTrigger.EnterActions>
                                <DataTrigger.ExitActions>
                                    <BeginStoryboard>
                                        <Storyboard>
                                            <ThicknessAnimation
                                                FillBehavior="Stop"
                                                Storyboard.TargetProperty="Margin"
                                                From="0,0,0,0"
                                                To="-440,0,0,0"
                                                Duration="0:0:0.6">
                                                <ThicknessAnimation.EasingFunction>
                                                    <CubicEase EasingMode="EaseOut" />
                                                </ThicknessAnimation.EasingFunction>
                                            </ThicknessAnimation>
                                        </Storyboard>
                                    </BeginStoryboard>
                                </DataTrigger.ExitActions>
                            </DataTrigger>
                        </Style.Triggers>
                    </Style>
                </Border.Style>
                <Grid>
                    <ui:ScrollViewerEx
                        Name="SettingsPanelScrollViewer"
                        Margin="0,80,0,50"
                        ui:ThemeManager.RequestedTheme="Dark"
                        ManipulationBoundaryFeedback="SCManipulationBoundaryFeedback"
                        PanningMode="VerticalOnly"
                        VerticalScrollBarVisibility="Auto">
                        <StackPanel Margin="20,20,20,20">
                            <Border
                                Height="100"
                                Margin="0,0,0,10"
                                BorderBrush="#a1a1aa"
                                BorderThickness="1"
                                CornerRadius="5">
                                <ikw:SimpleStackPanel VerticalAlignment="Center">
                                    <TextBlock
                                        Margin="0,0,0,10"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        FontSize="15"
                                        Foreground="#fafafa"
                                        Text="设置更改将自动保存，部分设置需要重启软件后生效" />
                                    <ikw:SimpleStackPanel Spacing="5">
                                        <ikw:SimpleStackPanel
                                            HorizontalAlignment="Center"
                                            Orientation="Horizontal"
                                            Spacing="5">
                                            <Button
                                                Width="116"
                                                Height="45"
                                                Click="BtnRestart_Click"
                                                FontFamily="Microsoft YaHei UI">
                                                <StackPanel
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    Orientation="Horizontal">
                                                    <ui:FontIcon Icon="{x:Static ui:SegoeFluentIcons.Refresh}" />
                                                    <Label
                                                        Margin="6,0,0,0"
                                                        VerticalAlignment="Center"
                                                        FontFamily="Microsoft YaHei UI"
                                                        FontSize="16"
                                                        FontWeight="Bold"
                                                        Foreground="#fafafa">
                                                        重启
                                                    </Label>
                                                </StackPanel>
                                            </Button>
                                            <Button
                                                Width="116"
                                                Height="45"
                                                Click="BtnResetToSuggestion_Click"
                                                FontFamily="Microsoft YaHei UI">
                                                <StackPanel
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    Orientation="Horizontal">
                                                    <ui:FontIcon FontSize="20" Icon="{x:Static ui:FluentSystemIcons.History_20_Regular}" />
                                                    <Label
                                                        Margin="6,0,0,0"
                                                        VerticalAlignment="Center"
                                                        FontFamily="Microsoft YaHei UI"
                                                        FontSize="16">
                                                        重置
                                                    </Label>
                                                </StackPanel>
                                            </Button>
                                            <Button
                                                Width="116"
                                                Height="45"
                                                Click="BtnExit_Click"
                                                FontFamily="Microsoft YaHei UI">
                                                <StackPanel
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    Orientation="Horizontal">
                                                    <ui:FontIcon Icon="{x:Static ui:SegoeFluentIcons.PowerButton}" />
                                                    <Label
                                                        Margin="6,0,0,0"
                                                        VerticalAlignment="Center"
                                                        FontFamily="Microsoft YaHei UI"
                                                        FontSize="16"
                                                        FontWeight="Bold"
                                                        Foreground="#fafafa">
                                                        退出
                                                    </Label>
                                                </StackPanel>
                                            </Button>
                                        </ikw:SimpleStackPanel>
                                    </ikw:SimpleStackPanel>
                                </ikw:SimpleStackPanel>
                            </Border>
                            <GroupBox>
                                <GroupBox.Header>
                                    <TextBlock
                                        Margin="0,12,0,0"
                                        FontSize="26"
                                        FontWeight="Bold"
                                        Foreground="#fafafa"
                                        Text="启动" />
                                </GroupBox.Header>
                                <ikw:SimpleStackPanel Spacing="6">
                                    <controls:SettingsControl Header="开机自启" IsOn="{Binding Settings.IsAutoStartEnabled}" />

                                    <controls:SettingsControl Header="软件启动后自动隐藏" IsOn="{Binding Settings.IsHideFloatingBarOnStart}" />

                                    <controls:SettingsControl
                                        Header="使窗口永不激活（无焦点模式）"
                                        Description="懂得都懂，不懂的不用开。此设置需要重启生效！"
                                        IsOn="{Binding Settings.IsWindowNoActivate}" />
                                </ikw:SimpleStackPanel>
                            </GroupBox>
                            <GroupBox>
                                <GroupBox.Header>
                                    <TextBlock
                                        Margin="0,12,0,0"
                                        FontSize="26"
                                        FontWeight="Bold"
                                        Foreground="#fafafa"
                                        Text="画板和墨迹" />
                                </GroupBox.Header>
                                <ikw:SimpleStackPanel Spacing="6">
                                    <controls:SettingsControl
                                        Header="显示画笔光标"
                                        IsOn="{Binding Settings.IsShowCursor}"
                                        Toggled="ToggleSwitchShowCursor_Toggled" />

                                    <controls:SettingsControl Header="橡皮大小" Description="# 非实时切换，下一次使用面积擦时生效。">
                                        <controls:SettingsControl.Switcher>
                                            <ComboBox SelectedIndex="{Binding Settings.EraserSize, Mode=TwoWay}">
                                                <ComboBoxItem Content="很小" FontFamily="Microsoft YaHei UI" />
                                                <ComboBoxItem Content="较小" FontFamily="Microsoft YaHei UI" />
                                                <ComboBoxItem Content="中等" FontFamily="Microsoft YaHei UI" />
                                                <ComboBoxItem Content="较大" FontFamily="Microsoft YaHei UI" />
                                                <ComboBoxItem Content="很大" FontFamily="Microsoft YaHei UI" />
                                            </ComboBox>
                                        </controls:SettingsControl.Switcher>
                                    </controls:SettingsControl>

                                    <Line
                                        Margin="0,4,0,4"
                                        HorizontalAlignment="Center"
                                        Stroke="#3f3f46"
                                        StrokeThickness="1"
                                        X1="0"
                                        X2="400"
                                        Y1="0"
                                        Y2="0" />

                                    <controls:SettingsControl
                                        Header="退出画板模式后隐藏墨迹"
                                        Description="# 开启 退出画板模式后隐藏墨迹 选项后，进入 PPT 模式时未处于批注模式时不会显示墨迹。"
                                        IsOn="{Binding Settings.HideStrokeWhenSelecting}" />

                                    <controls:SettingsControl Header="清空墨迹时删除墨迹历史记录" IsOn="{Binding Settings.ClearCanvasAndClearTimeMachine}" />

                                    <Line
                                        Margin="0,4,0,4"
                                        HorizontalAlignment="Center"
                                        Stroke="#3f3f46"
                                        StrokeThickness="1"
                                        X1="0"
                                        X2="400"
                                        Y1="0"
                                        Y2="0" />
                                    <controls:SettingsControl Header="让墨迹使用贝塞尔曲线平滑处理" IsOn="{Binding Settings.FitToCurve}" />
                                </ikw:SimpleStackPanel>
                            </GroupBox>
                            <GroupBox>
                                <GroupBox.Header>
                                    <TextBlock
                                        Margin="0,12,0,0"
                                        FontSize="26"
                                        FontWeight="Bold"
                                        Foreground="#fafafa"
                                        Text="手势" />
                                </GroupBox.Header>
                                <ikw:SimpleStackPanel Spacing="6">
                                    <controls:SettingsControl
                                        Header="进退白板模式自动开关双指移动功能"
                                        Description="# 开启后退出画板模式时自动关闭双指移动手势，进入白板模式时自动开启双指移动手势"
                                        IsOn="{Binding Settings.AutoSwitchTwoFingerGesture}" />
                                    <Line
                                        Margin="0,4,0,4"
                                        HorizontalAlignment="Center"
                                        Stroke="#3f3f46"
                                        StrokeThickness="1"
                                        X1="0"
                                        X2="400"
                                        Y1="0"
                                        Y2="0" />
                                    <controls:SettingsControl
                                        Header="允许双指旋转与缩放选中的墨迹"
                                        Description="# 允许选中墨迹后对墨迹进行双指或多指缩放操作（此设置不受“允许双指旋转”设置的影响）"
                                        IsOn="{Binding Settings.IsEnableTwoFingerRotationOnSelection}" />
                                </ikw:SimpleStackPanel>
                            </GroupBox>
                            <GroupBox Name="GroupBoxAppearanceNewUI">
                                <GroupBox.Header>
                                    <TextBlock
                                        Margin="0,12,0,0"
                                        FontSize="26"
                                        FontWeight="Bold"
                                        Foreground="#fafafa"
                                        Text="个性化" />
                                </GroupBox.Header>
                                <ikw:SimpleStackPanel Spacing="6">
                                    <controls:SettingsControl Header="浮动工具栏图标">
                                        <controls:SettingsControl.Switcher>
                                            <ComboBox SelectedIndex="{Binding Settings.FloatingBarImg, Mode=TwoWay}">
                                                <ComboBoxItem Content="ICC-Re" />
                                                <ComboBoxItem Content="怀旧版蓝色 ICC" />
                                            </ComboBox>
                                        </controls:SettingsControl.Switcher>
                                    </controls:SettingsControl>
                                    <controls:SettingsControl Header="浮动工具栏缩放">
                                        <controls:SettingsControl.Switcher>
                                            <StackPanel Orientation="Horizontal">
                                                <Slider
                                                    Width="168"
                                                    AutoToolTipPlacement="None"
                                                    FontFamily="Microsoft YaHei UI"
                                                    FontSize="20"
                                                    IsSnapToTickEnabled="True"
                                                    Maximum="1.25"
                                                    Minimum="0.5"
                                                    TickFrequency="0.05"
                                                    TickPlacement="None"
                                                    Value="{Binding Settings.ViewboxFloatingBarScaleTransformValue, Mode=TwoWay}" />
                                                <TextBlock
                                                    Margin="12,0,16,0"
                                                    VerticalAlignment="Center"
                                                    FontFamily="Consolas"
                                                    FontSize="14"
                                                    Text="{Binding Settings.ViewboxFloatingBarScaleTransformValue, StringFormat={}{0:F2}}" />
                                            </StackPanel>
                                        </controls:SettingsControl.Switcher>
                                    </controls:SettingsControl>
                                    <controls:SettingsControl Header="浮动工具栏透明度">
                                        <controls:SettingsControl.Switcher>
                                            <StackPanel Orientation="Horizontal">
                                                <Slider
                                                    Width="168"
                                                    AutoToolTipPlacement="None"
                                                    FontFamily="Microsoft YaHei UI"
                                                    FontSize="20"
                                                    IsSnapToTickEnabled="True"
                                                    Maximum="1"
                                                    Minimum="0.3"
                                                    TickFrequency="0.05"
                                                    TickPlacement="None"
                                                    Value="{Binding Settings.ViewboxFloatingBarOpacityValue, Mode=TwoWay}" />
                                                <TextBlock
                                                    Margin="12,0,16,0"
                                                    VerticalAlignment="Center"
                                                    FontFamily="Consolas"
                                                    FontSize="14"
                                                    Text="{Binding Settings.ViewboxFloatingBarOpacityValue, StringFormat={}{0:F2}}" />
                                            </StackPanel>
                                        </controls:SettingsControl.Switcher>
                                    </controls:SettingsControl>
                                    <controls:SettingsControl Header="浮栏在PPT下透明度">
                                        <controls:SettingsControl.Switcher>
                                            <StackPanel Orientation="Horizontal">
                                                <Slider
                                                    Width="168"
                                                    AutoToolTipPlacement="None"
                                                    FontFamily="Microsoft YaHei UI"
                                                    FontSize="20"
                                                    IsSnapToTickEnabled="True"
                                                    Maximum="1"
                                                    Minimum="0.3"
                                                    TickFrequency="0.05"
                                                    TickPlacement="None"
                                                    Value="{Binding Settings.ViewboxFloatingBarOpacityInPPTValue, Mode=TwoWay}" />
                                                <TextBlock
                                                    Margin="12,0,16,0"
                                                    VerticalAlignment="Center"
                                                    FontFamily="Consolas"
                                                    FontSize="14"
                                                    Text="{Binding Settings.ViewboxFloatingBarOpacityInPPTValue, StringFormat={}{0:F2}}" />
                                            </StackPanel>
                                        </controls:SettingsControl.Switcher>
                                    </controls:SettingsControl>
                                    <Line
                                        Margin="0,4,0,4"
                                        HorizontalAlignment="Center"
                                        Stroke="#3f3f46"
                                        StrokeThickness="1"
                                        X1="0"
                                        X2="400"
                                        Y1="0"
                                        Y2="0" />
                                    <controls:SettingsControl Header="在调色盘窗口中显示 笔尖模式 按钮" IsOn="{Binding Settings.IsEnableDisPlayNibModeToggler}" />
                                    <Line
                                        Margin="0,4,0,4"
                                        HorizontalAlignment="Center"
                                        Stroke="#3f3f46"
                                        StrokeThickness="1"
                                        X1="0"
                                        X2="400"
                                        Y1="0"
                                        Y2="0" />
                                    <controls:SettingsControl Header="白板 UI 80% 缩放" IsOn="{Binding Settings.EnableViewboxBlackBoardScaleTransform}" />
                                    <controls:SettingsControl Header="在白板中显示当前时间和日期" IsOn="{Binding Settings.EnableTimeDisplayInWhiteboardMode}" />
                                    <Line
                                        Margin="0,4,0,4"
                                        HorizontalAlignment="Center"
                                        Stroke="#3f3f46"
                                        StrokeThickness="1"
                                        X1="0"
                                        X2="400"
                                        Y1="0"
                                        Y2="0" />
                                    <controls:SettingsControl Header="取消收纳按钮图标">
                                        <controls:SettingsControl.Switcher>
                                            <ComboBox SelectedIndex="{Binding Settings.UnFoldButtonImageType, Mode=TwoWay}">
                                                <ComboBoxItem Content="箭头" FontFamily="Microsoft YaHei UI" />
                                                <ComboBoxItem Content="笔" FontFamily="Microsoft YaHei UI" />
                                            </ComboBox>
                                        </controls:SettingsControl.Switcher>
                                    </controls:SettingsControl>
                                    <Line
                                        Margin="0,4,0,4"
                                        HorizontalAlignment="Center"
                                        Stroke="#3f3f46"
                                        StrokeThickness="1"
                                        X1="0"
                                        X2="400"
                                        Y1="0"
                                        Y2="0" />
                                    <Border
                                        Height="50"
                                        Margin="-20,0"
                                        Padding="20,0"
                                        Background="#101010">
                                        <Grid>
                                            <ikw:SimpleStackPanel FlowDirection="RightToLeft" Orientation="Horizontal">
                                                <ui:FontIcon FontSize="20" Icon="{x:Static ui:SegoeFluentIcons.Volume}" />
                                                <ui:FontIcon
                                                    Margin="8,0,0,0"
                                                    FontSize="20"
                                                    Icon="{x:Static ui:SegoeFluentIcons.Ethernet}" />
                                                <Image
                                                    Name="ICCTrayIconExampleImage"
                                                    Width="24"
                                                    Height="24"
                                                    Margin="8,0,0,0"
                                                    Source="/Resources/ICC-Re.ico"
                                                    Visibility="{Binding Settings.EnableTrayIcon, Converter={StaticResource BooleanToVisibilityConverter}}" />
                                            </ikw:SimpleStackPanel>
                                            <TextBlock
                                                Margin="12,0,0,0"
                                                VerticalAlignment="Center"
                                                FontSize="20"
                                                FontWeight="Bold"
                                                Foreground="#fafafa"
                                                Text="任务栏托盘图标" />
                                        </Grid>
                                    </Border>
                                    <controls:SettingsControl Header="启用托盘图标" IsOn="{Binding Settings.EnableTrayIcon}" />
                                </ikw:SimpleStackPanel>
                            </GroupBox>
                            <GroupBox>
                                <GroupBox.Header>
                                    <TextBlock
                                        Margin="0,12,0,0"
                                        FontSize="26"
                                        FontWeight="Bold"
                                        Foreground="#fafafa"
                                        Text="PPT联动" />
                                </GroupBox.Header>
                                <ikw:SimpleStackPanel Spacing="6">
                                    <TextBlock
                                        FontSize="12"
                                        Foreground="#a1a1aa"
                                        Text="此部分的设置项将会在幻灯片播放时使用，优先级高于其他设置项" />
                                    <ikw:SimpleStackPanel HorizontalAlignment="Left" Orientation="Horizontal">
                                        <Image
                                            Width="28"
                                            Height="28"
                                            Margin="0,0,6,0"
                                            VerticalAlignment="Center"
                                            Source="/Resources/Icons-png/Powerpoint.png" />
                                        <TextBlock
                                            Margin="0,0,16,0"
                                            VerticalAlignment="Center"
                                            FontSize="14"
                                            Foreground="#fafafa"
                                            Text="Microsoft PowerPoint 支持" />
                                        <ui:ToggleSwitch
                                            Name="ToggleSwitchSupportPowerPoint"
                                            FontFamily="Microsoft YaHei UI"
                                            FontWeight="Bold"
                                            IsOn="{Binding Settings.PowerPointSupport, Mode=TwoWay}"
                                            OffContent=""
                                            OnContent=""
                                            Toggled="ToggleSwitchSupportPowerPoint_Toggled" />
