                        </Grid>
                    </Border>
                    <Border
                        x:Name="EnableTwoFingerGestureBorder"
                        Width="36"
                        Height="36"
                        Margin="2,0,0,0"
                        Background="#fafafa"
                        BorderBrush="#9952525b"
                        BorderThickness="1"
                        CornerRadius="5"
                        Visibility="Visible">
                        <ikw:SimpleStackPanel
                            Width="36"
                            Margin="0"
                            HorizontalAlignment="Center"
                            Background="Transparent"
                            MouseUp="TwoFingerGestureBorder_MouseUp"
                            Orientation="Vertical">
                            <Image
                                x:Name="EnableTwoFingerGestureBtn"
                                Width="28"
                                Height="18"
                                Margin="0,3,0,0"
                                RenderOptions.BitmapScalingMode="HighQuality"
                                Source="/Resources/new-icons/gesture.png" />
                            <TextBlock
                                x:Name="gestureiconText"
                                Margin="0,0,0,0"
                                FontSize="8"
                                Foreground="Black"
                                Text="手势"
                                TextAlignment="Center" />
                        </ikw:SimpleStackPanel>
                    </Border>
                    <Grid Width="0">
                        <Border
                            Name="TwoFingerGestureBorder"
                            Margin="-86,-111,-33,40"
                            Background="#fafafa"
                            BorderBrush="#2563eb"
                            BorderThickness="1"
                            ClipToBounds="True"
                            CornerRadius="5"
                            Opacity="1"
                            Visibility="Collapsed">
                            <ikw:SimpleStackPanel Margin="0">
                                <Border
                                    Margin="-1,-1,-1,1"
                                    Background="#2563eb"
                                    BorderBrush="#1e3a8a"
                                    BorderThickness="0,0,0,1"
                                    CornerRadius="6,6,0,0">
                                    <Canvas Height="24" ClipToBounds="True">
                                        <TextBlock
                                            Canvas.Left="8"
                                            Padding="0,5"
                                            FontSize="11"
                                            FontWeight="Bold"
                                            Foreground="White"
                                            Text="手势选项"
                                            TextAlignment="Center" />
                                    </Canvas>
                                </Border>
                                <ikw:SimpleStackPanel
                                    Margin="0,3,0,0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Orientation="Horizontal"
                                    Spacing="4">
                                    <Image
                                        Width="16"
                                        Height="16"
                                        RenderOptions.BitmapScalingMode="HighQuality"
                                        Source="/Resources/new-icons/multi-touch.png" />
                                    <Label
                                        Content="多指书写"
                                        VerticalAlignment="Center"
                                        FontSize="10" />
                                    <Viewbox
                                        Width="36"
                                        Height="18"
                                        Margin="4,0,0,0">
                                        <ui:ToggleSwitch
                                            Name="ToggleSwitchEnableMultiTouchMode"
                                            MinWidth="0"
                                            Margin="0,-6,0,-6"
                                            FontFamily="Microsoft YaHei UI"
                                            IsOn="False"
                                            OffContent=""
                                            OnContent=""
                                            RenderTransformOrigin="0.5,0.5"
                                            Toggled="ToggleSwitchEnableMultiTouchMode_Toggled">
                                            <ui:ToggleSwitch.RenderTransform>
                                                <TransformGroup>
                                                    <ScaleTransform />
                                                    <SkewTransform />
                                                    <RotateTransform />
                                                    <TranslateTransform X="8" />
                                                </TransformGroup>
                                            </ui:ToggleSwitch.RenderTransform>
                                        </ui:ToggleSwitch>
                                    </Viewbox>
                                </ikw:SimpleStackPanel>
                                <ikw:SimpleStackPanel
                                    x:Name="TwoFingerGestureSimpleStackPanel"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Opacity="1"
                                    Orientation="Horizontal"
                                    Spacing="4">
                                    <Image
                                        Width="16"
                                        Height="16"
                                        RenderOptions.BitmapScalingMode="HighQuality"
                                        Source="/Resources/new-icons/hand-move.png" />
                                    <Label
                                        Content="双指移动"
                                        VerticalAlignment="Center"
                                        FontSize="10" />
                                    <Viewbox
                                        Width="36"
                                        Height="18"
                                        Margin="4,0,0,0">
                                        <ui:ToggleSwitch
                                            Name="ToggleSwitchEnableTwoFingerTranslate"
                                            MinWidth="0"
                                            Margin="0,-6,0,-6"
                                            FontFamily="Microsoft YaHei UI"
                                            IsOn="False"
                                            OffContent=""
                                            OnContent=""
                                            RenderTransformOrigin="0.5,0.5"
                                            Toggled="ToggleSwitchEnableTwoFingerTranslate_Toggled">
                                            <ui:ToggleSwitch.RenderTransform>
                                                <TransformGroup>
                                                    <ScaleTransform />
                                                    <SkewTransform />
                                                    <RotateTransform />
                                                    <TranslateTransform X="8" />
                                                </TransformGroup>
                                            </ui:ToggleSwitch.RenderTransform>
                                        </ui:ToggleSwitch>
                                    </Viewbox>
                                </ikw:SimpleStackPanel>
                                <ikw:SimpleStackPanel
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Opacity="{Binding ElementName=TwoFingerGestureSimpleStackPanel, Path=Opacity}"
                                    Orientation="Horizontal"
                                    Spacing="4">
                                    <Image
                                        Width="16"
                                        Height="16"
                                        RenderOptions.BitmapScalingMode="HighQuality"
                                        Source="/Resources/new-icons/zoom.png" />
                                    <Label
                                        Content="双指缩放"
                                        VerticalAlignment="Center"
                                        FontSize="10" />
                                    <Viewbox
                                        Width="36"
                                        Height="18"
                                        Margin="4,0,0,0">
                                        <ui:ToggleSwitch
                                            Name="ToggleSwitchEnableTwoFingerZoom"
                                            MinWidth="0"
                                            Margin="0,-6,0,-6"
                                            FontFamily="Microsoft YaHei UI"
                                            IsOn="False"
                                            OffContent=""
                                            OnContent=""
                                            RenderTransformOrigin="0.5,0.5"
                                            Toggled="ToggleSwitchEnableTwoFingerZoom_Toggled">
                                            <ui:ToggleSwitch.RenderTransform>
                                                <TransformGroup>
                                                    <ScaleTransform />
                                                    <SkewTransform />
                                                    <RotateTransform />
                                                    <TranslateTransform X="8" />
                                                </TransformGroup>
                                            </ui:ToggleSwitch.RenderTransform>
                                        </ui:ToggleSwitch>
                                    </Viewbox>
                                </ikw:SimpleStackPanel>
                                <ikw:SimpleStackPanel
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Opacity="{Binding ElementName=TwoFingerGestureSimpleStackPanel, Path=Opacity}"
                                    Orientation="Horizontal"
                                    Spacing="4">
                                    <Image
                                        Width="16"
                                        Height="16"
                                        RenderOptions.BitmapScalingMode="HighQuality"
                                        Source="/Resources/new-icons/rotate.png" />
                                    <Label
                                        Content="双指旋转"
                                        VerticalAlignment="Center"
                                        FontSize="10" />
                                    <Viewbox
                                        Width="36"
                                        Height="18"
                                        Margin="4,0,0,0">
                                        <ui:ToggleSwitch
                                            MinWidth="0"
                                            Margin="0,-6,0,-6"
                                            FontFamily="Microsoft YaHei UI"
                                            IsOn="{Binding Settings.IsEnableTwoFingerRotation, Mode=TwoWay}"
                                            OffContent=""
                                            OnContent=""
                                            RenderTransformOrigin="0.5,0.5">
                                            <ui:ToggleSwitch.RenderTransform>
                                                <TransformGroup>
                                                    <ScaleTransform />
                                                    <SkewTransform />
                                                    <RotateTransform />
                                                    <TranslateTransform X="8" />
                                                </TransformGroup>
                                            </ui:ToggleSwitch.RenderTransform>
                                        </ui:ToggleSwitch>
                                    </Viewbox>

                                </ikw:SimpleStackPanel>
                            </ikw:SimpleStackPanel>
                        </Border>
                    </Grid>
                    <Border
                        x:Name="BorderFloatingBarExitPPTBtn"
                        Width="34"
                        Height="36"
                        Margin="2,0,0,0"
                        Background="{DynamicResource FloatBarBackground}"
                        BorderBrush="{DynamicResource FloatBarBorderBrush}"
                        BorderThickness="1"
                        CornerRadius="4"
                        MouseDown="Border_MouseDown"
                        MouseUp="ImagePPTControlEnd_MouseUp"
                        Visibility="{Binding PowerPointService.IsInSlideShow, Converter={StaticResource BooleanToVisibilityConverter}}">
                        <ikw:SimpleStackPanel
                            Width="28"
                            Margin="0,0"
                            HorizontalAlignment="Center"
                            Background="Transparent"
                            Orientation="Vertical">
                            <Image
                                Width="28"
                                Height="17"
                                Margin="0,3,0,0"
                                RenderOptions.BitmapScalingMode="HighQuality"
                                Source="/Resources/new-icons/end-slides-show.png" />
                            <TextBlock
                                Margin="0,1,0,0"
                                FontSize="8"
                                Foreground="Black"
                                Text="退出"
                                TextAlignment="Center" />
                        </ikw:SimpleStackPanel>
                    </Border>
                </ikw:SimpleStackPanel>
            </Viewbox>
        </Grid>

        <Grid
            Name="GridForFloatingBarDraging"
            Background="#01000000"
            MouseMove="SymbolIconEmoji_MouseMove"
            MouseUp="SymbolIconEmoji_MouseUp"
            Visibility="Collapsed" />

        <!--  // PPT 翻页按钮面板 //  -->
        <controls:PPTNavigationPanel
            x:Name="PPTNavigationPanel"
            DataContext="{Binding}"
            NextClick="PPTNavigationPanel_NextClick"
            PageClick="PPTNavigationPanel_PageClick"
            PanelWidth="{Binding Settings.PPTNavigationPanelWidth}"
            PreviousClick="PPTNavigationPanel_PreviousClick">
            <controls:PPTNavigationPanel.Style>
                <Style TargetType="controls:PPTNavigationPanel">
                    <Setter Property="Visibility" Value="Collapsed" />
                    <Style.Triggers>
                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding PowerPointService.IsInSlideShow}" Value="True" />
                                <Condition Binding="{Binding Settings.ShowPPTButton}" Value="True" />
                                <Condition Binding="{Binding IsFloatingBarVisible}" Value="True" />
                                <Condition Binding="{Binding AppMode}" Value="Normal" />
                            </MultiDataTrigger.Conditions>
                            <MultiDataTrigger.Setters>
                                <Setter Property="Visibility" Value="Visible" />
                            </MultiDataTrigger.Setters>
                            <MultiDataTrigger.EnterActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation
                                            Storyboard.TargetProperty="Opacity"
                                            From="0"
                                            To="1"
                                            Duration="0:0:0.3" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </MultiDataTrigger.EnterActions>
                        </MultiDataTrigger>
                    </Style.Triggers>
                </Style>
            </controls:PPTNavigationPanel.Style>
        </controls:PPTNavigationPanel>

        <!--  // 侧边栏 //  -->
        <Viewbox
            x:Name="LeftSidePanel"
            Width="32"
            Height="50"
            HorizontalAlignment="Left"
            VerticalAlignment="Center"
            MouseUp="SidePanelUnFoldButton_MouseUp"
            Style="{StaticResource SidePanelViewBoxStyle}"
            Tag="Left">
            <ikw:SimpleStackPanel>
                <Border
                    Width="32"
                    Height="50"
                    Background="#9918181b"
                    CornerRadius="0,25,25,0"
                    Opacity="1">
                    <Image
                        x:Name="LeftUnFoldBtnImgChevron"
                        Margin="10,0,0,0"
                        RenderOptions.BitmapScalingMode="HighQuality">
                        <Image.Style>
                            <Style TargetType="Image">
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding Settings.UnFoldButtonImageType}" Value="0">
                                        <Setter Property="Source">
                                            <Setter.Value>
                                                <BitmapImage UriSource="pack://application:,,,/Resources/new-icons/unfold-chevron.png" />
                                            </Setter.Value>
                                        </Setter>
                                        <Setter Property="Width" Value="14" />
                                        <Setter Property="Height" Value="14" />
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding Settings.UnFoldButtonImageType}" Value="1">
                                        <Setter Property="Source">
                                            <Setter.Value>
                                                <BitmapImage UriSource="pack://application:,,,/Resources/new-icons/pen-white.png" />
                                            </Setter.Value>
                                        </Setter>
                                        <Setter Property="Width" Value="18" />
                                        <Setter Property="Height" Value="18" />
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Image.Style>
                    </Image>
                </Border>
            </ikw:SimpleStackPanel>
        </Viewbox>
        <Viewbox
            x:Name="RightSidePanel"
            Width="32"
            Height="50"
            HorizontalAlignment="Right"
            VerticalAlignment="Center"
            MouseUp="SidePanelUnFoldButton_MouseUp"
            Style="{StaticResource SidePanelViewBoxStyle}"
            Tag="Right">
            <ikw:SimpleStackPanel>
                <Border
                    Width="32"
                    Height="50"
                    Background="#9918181b"
                    CornerRadius="25,0,0,25"
                    Opacity="1">
                    <Image
                        x:Name="RightUnFoldBtnImgChevron"
                        Margin="0,0,10,0"
                        RenderOptions.BitmapScalingMode="HighQuality"
                        RenderTransformOrigin="0.5,0.5">
                        <Image.Style>
                            <Style TargetType="Image">
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding Settings.UnFoldButtonImageType}" Value="0">
                                        <Setter Property="Source" Value="pack://application:,,,/Resources/new-icons/unfold-chevron.png" />
                                        <Setter Property="Width" Value="14" />
                                        <Setter Property="Height" Value="14" />
                                        <Setter Property="RenderTransform">
                                            <Setter.Value>
                                                <RotateTransform Angle="180" />
                                            </Setter.Value>
                                        </Setter>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding Settings.UnFoldButtonImageType}" Value="1">
                                        <Setter Property="Source" Value="pack://application:,,,/Resources/new-icons/pen-white.png" />
                                        <Setter Property="Width" Value="18" />
                                        <Setter Property="Height" Value="18" />
                                        <Setter Property="RenderTransform" Value="{x:Null}" />
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Image.Style>
                    </Image>
                </Border>
            </ikw:SimpleStackPanel>
        </Viewbox>
    </Grid>
</Window>